{
    "manifest_version": 3,
    "name": "LAMP",
    "author": "Cosmin Alexandru",
    "description": "LAMP is a basic SEO Audit Tool which allows its users to perform audits on different webpages.",
    "version": "0.3.0",

    "icons": {
        "16": "/icons/lamp_16px.png",
        "48": "/icons/lamp_48px.png",
        "128": "/icons/lamp_128px.png"
    },
    "background": {
        "service_worker": "main.js" 
    },
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"]
}